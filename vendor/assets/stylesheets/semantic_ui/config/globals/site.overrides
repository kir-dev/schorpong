/*******************************
         Site Overrides
*******************************/

.page-header {
  background-image: url('/images/header.png');
  background-position: center top;
  background-attachment: fixed;
  height: 300px;
  position: relative;
}
.page-header svg {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 2vw;
}
.page-header .logo {
  position:absolute;
  left: 50%;
  margin-left: -70px;
  bottom:100px
}
.page-header .logo img {
  height: 140px;
  width: 140px;
  position:absolute;
}